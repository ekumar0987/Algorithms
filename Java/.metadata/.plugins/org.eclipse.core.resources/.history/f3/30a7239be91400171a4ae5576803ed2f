/*
 * Implement a method to rotate a NxN matrix
 */
public class matrixRotation {
	
	public int[][] transposeMatrix(int[][] m){
		
		int tmp;
		for(int i=0; i<m.length; i++){
			for(int j=0; j<i; j++){
				tmp = m[i][j];
				m[i][j] = m[j][i];
				m[j][i] = tmp;
			}
		}
	}
	
	public int[][] rotateMatrix(int[][] m){
		
	}
	
	
	
	public static void main(String[] args) {

	}

}
