package datastructures;

public class BinarySearchTrees {

	Node root;
	
	public BinarySearchTrees(){
		root = null;
	}
	
	public class Node{
		
		int data;
		Node left;
		Node right;
		
		public Node(int data){
			this.data = data;
			this.left = null;
			this.right = null;
		}
		
	}
	
	public static void main(String[] args) {
		

	}

}
